package com.shivam.movie_catalog_service.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.Id;

@Entity
public class MovieInfo {

	@Id
	@GeneratedValue
	private Long id;
	
	private String name;
	
	private String description;
	
	private String path;
	
	
	
}
